{
  "version": "0.1.0",
  "description": "Simple Snap to generate a secret key with ECDH",
  "proposedName": "ECDH Snap",
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/template-snap-monorepo.git"
  },
  "source": {
    "shasum": "rRT2QQ8QfxN3eNN1RXtvFw9y3hYL7vl4xJpzTmN/TyQ=",
    "location": {
      "npm": {
        "filePath": "dist/bundle.js",
        "iconPath": "images/icon.svg",
        "packageName": "snap",
        "registry": "https://registry.npmjs.org/"
      }
    }
  },
  "initialPermissions": {
    "snap_dialog": {},
    "snap_getEntropy": {},
    "snap_notify": {},
    "snap_getBip44Entropy": [
      {
        "coinType": 1
      }
    ],
    "snap_getBip32PublicKey": [
      {
        "path": [
          "m",
          "44'",
          "1'",
          "0'",
          "0",
          "0"
        ],
        "curve": "secp256k1"
      }
    ],
    "endowment:rpc": {
      "dapps": true,
      "snaps": false
    }
  },
  "manifestVersion": "0.1"
}
